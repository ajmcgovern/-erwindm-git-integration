        /*  %JoinFKPK(Sales.SalesOrderHeader,deleted," = "," AND") */
        Sales.SalesOrderHeader.TerritoryID = deleted.TerritoryID
    )
    BEGIN
      SELECT @errno  = 30005,
             @errmsg = 'Cannot update Sales.SalesTerritory because Sales.SalesOrderHeader exists.'
      GOTO error
