CREATE TRIGGER Production.tU_ProductDocument ON Production.ProductDocument FOR UPDATE AS
/* erwin Builtin Trigger */
/* UPDATE trigger on ProductDocument */
BEGIN
  DECLARE  @numrows int,
           @nullcnt int,
           @validcnt int,
           @insProductID int, 
           @insDocumentNode hierarchyid,
           @errno   int,
           @severity int,
           @state    int,
           @errmsg  varchar(255)

