      SELECT * FROM deleted,HumanResources.EmployeeDepartmentHistory
      WHERE
        /*  %JoinFKPK(HumanResources.EmployeeDepartmentHistory,deleted," = "," AND") */
        HumanResources.EmployeeDepartmentHistory.DepartmentID = deleted.DepartmentID
