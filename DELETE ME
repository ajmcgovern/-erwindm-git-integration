      SELECT * FROM deleted,Purchasing.Vendor
      WHERE
        /*  %JoinFKPK(Purchasing.Vendor,deleted," = "," AND") */
        Purchasing.Vendor.BusinessEntityID = deleted.BusinessEntityID
