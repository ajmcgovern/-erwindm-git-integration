    /* %ParentPK(" OR",UPDATE) */
    UPDATE(UnitMeasureCode)
  BEGIN
    IF EXISTS (
      SELECT * FROM deleted,Production.Product
      WHERE
        /*  %JoinFKPK(Production.Product,deleted," = "," AND") */
        Production.Product.WeightUnitMeasureCode = deleted.UnitMeasureCode
    )
