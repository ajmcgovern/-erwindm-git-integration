    CHILD_OWNER="Sales", CHILD_TABLE="SalesOrderHeaderSalesReason"
    P2C_VERB_PHRASE="", C2P_VERB_PHRASE="", 
    FK_CONSTRAINT="FK_SalesOrderHeaderSalesReason_SalesReason_SalesReasonID", FK_COLUMNS="SalesReasonID" */
