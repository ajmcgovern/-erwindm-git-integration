CREATE TABLE [JobCandidate]
( 
	[JobCandidateID]     int  NOT NULL  IDENTITY ( 1,1 ) ,
	[BusinessEntityID]   int  NULL ,
	[Resume]             xml ( CONTENT [HRResumeSchemaCollection] ) NULL ,
