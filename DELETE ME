    IF EXISTS (SELECT * FROM deleted,Production.ProductModel
      WHERE
        /* %JoinFKPK(deleted,Production.ProductModel," = "," AND") */
        deleted.ProductModelID = Production.ProductModel.ProductModelID AND
        NOT EXISTS (
