CREATE TRIGGER Sales.Trigger_3094 ON Sales.SalesOrderHeader FOR UPDATE AS
/* erwin Builtin Trigger */
/* UPDATE trigger on SalesOrderHeader */
BEGIN
  DECLARE  @numrows int,
           @nullcnt int,
           @validcnt int,
           @insSalesOrderID int,
